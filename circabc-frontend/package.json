{
  "name": "circabc",
  "version": "4.2.4.3",
  "license": "EUPL-1.1",
  "engines": {
    "node": "^18.13.0 || ^20.9.0",
    "yarn": ">=1.22.4 <2"
  },
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "nx-migrate": "nx migrate latest",
    "ng-version": "ng version",
    "ng-update": "ng update @angular/core @angular/cli --next --force  --allow-dirty",
    "ng-update-cli": "ng update @angular/cli --from 9 --to 10 --migrate-only --allow-dirty",
    "ng-update-core": "ng update @angular/core --from 9 --to 10 --migrate-only --allow-dirty",
    "ng-update-one": "ng update  @angular/core --migrate-only migration-v9-module-with-providers --allow-dirty",
    "ng-update-help": "ng update --help",
    "start": "ng serve -o",
    "build": "ng build",
    "build-prod-profile": "ng build   --profile",
    "build-prod": "ng build ",
    "dcp-npx": "npx jscpd  src --format \"typescript,markup,scss\"  --ignore \"src/app/core/generated/**/*.ts\" --min-lines 30 --max-lines 2000 ",
    "dcp": "jscpd  src --format \"typescript,markup,scss\"  --ignore \"src/app/core/generated/**/*.ts\" --min-lines 30 --max-lines 2000 ",
    "gulp-format": "gulp  prettier",
    "gulp-format-scss": "gulp  format:scss ",
    "format-check": "prettier --single-quote --list-different --html-whitespace-sensitivity strict \"src/**/*.html\" \"src/**/*.ts\" ",
    "format-fix": "prettier --write --single-quote --list-different --html-whitespace-sensitivity strict \"src/**/*.html\" \"src/**/*.ts\" ",
    "format-fix-crlf": "prettier  --write --single-quote --end-of-line crlf --list-different --html-whitespace-sensitivity strict \"src/**/*.html\" \"src/**/*.ts\" ",
    "format-fix-gen": "prettier --write --single-quote --list-different \"src/app/core/generated/**/*.ts\" ",
    "format-check-crlf": "prettier --single-quote --list-different --end-of-line crlf \"src/**/*.ts\" ",
    "format-check-scss": "prettier --single-quote --list-different  \"src/**/*.scss\" ",
    "format-fix-scss": "prettier --write --single-quote --list-different  \"src/**/*.scss\" ",
    "prettier": "prettier --version",
    "lf-gen": "crlf --set=LF \"src/app/core/generated/**/*.ts",
    "lf-all": "crlf --set=LF \"src/**/*.ts",
    "image-compress": "gulp imagemin",
    "tsc-version": "tsc  --version",
    "tsc": "tsc  --project src/tsconfig.app.json --pretty",
    "tsc-watch": "tsc  --project src/tsconfig.app.json --pretty --watch",
    "tsc-heap": "node --max-old-space-size=8192 ./node_modules/typescript/lib/tsc.js  --project src/tsconfig.app.json --pretty",
    "stylelint": "node ./node_modules/stylelint/bin/stylelint.mjs \"src/**/*.scss\" --custom-syntax postcss-scss ",
    "stylelint-html": "node ./node_modules/stylelint/bin/stylelint.mjs \"src/**/*.html\" ",
    "stylelint-fix": "node ./node_modules/stylelint/bin/stylelint.mjs \"src/**/*.scss\" --custom-syntax postcss-scss --fix",
    "stylelint-html-fix": "node ./node_modules/stylelint/bin/stylelint.mjs \"src/**/*.html\" --fix ",
    "lint-one-line": "ng lint circabc --format ./eslint-formatter.js",
    "lint": "ng lint circabc --format stylish",
    "lint-force": "ng lint circabc --force  --format stylish",
    "lint-fix": "ng lint circabc --fix --force  --format stylish",
    "lint-biome": "biome check ./src",
    "lint-fix-biome": "biome check ./src --write --unsafe",
    "build-stats": "ng build --stats-json",
    "view-stats": "npx webpack-bundle-analyzer dist/circabc/stats.json",
    "doc-build": "npx compodoc -p tsconfig.json",
    "doc-build-heap": "npm run doc-build  --max-old-space-size=8192",
    "doc-serve": "npx http-server ./documentation -p 9090",
    "doc-build-serve": "npx compodoc -p tsconfig.json -s -r 9090 -o",
    "get-build-date": "node getBuildDate.js",
    "http-serve": "http-serve ./dist/circabc -p 4200",
    "http-serve-gzip": "http-serve ./dist/circabc -p 4200 --gzip ",
    "bamboo": "npm run get-build-date && npm run tsc && npm run format-check && npm run lint  && npm run stylelint && npm run build-prod ",
    "bamboo-biome": "npm run get-build-date && npm run tsc && npm run format-check && npm run lint && npm run lint-biome && npm run stylelint && npm run build-prod ",
    "bamboo-crlf": "npm run get-build-date && npm run tsc && npm run format-check-crlf && npm run lint && npm run stylelint && npm run build-prod ",
    "build-training": "npm run get-build-date && ng build -c=training",
    "localhost-serve": "npm run get-build-date && ng serve -c=localhost-serve -o",
    "localhost": "npm run get-build-date && ng build   -c=localhost",
    "docker": "npm run get-build-date && ng build   -c=docker && gzipper compress --verbose ./dist/circabc",
    "docker-it-nrw": "npm run get-build-date && ng build   -c=docker-it-nrw && gzipper compress --verbose ./dist/circabc",
    "build-tomcat": "npm run get-build-date && ng build -c=tomcat ",
    "tomcat": "npm run get-build-date && ng serve -c=tomcat -o",
    "next": "npm run get-build-date && ng serve -c=next -o",
    "next-docker": "npm run get-build-date && ng serve -c=next-docker --host 0.0.0.0 --poll 2000",
    "next-aws": "npm run get-build-date && ng serve -c=next-aws --host 0.0.0.0 --poll 2000",
    "ec2": "npm run get-build-date && ng serve -c=ec2 -o",
    "test": "npm run get-build-date && ng build   -c=test",
    "stress": "npm run get-build-date && ng build  -c=stress",
    "development": "npm run get-build-date && ng build  -c=development",
    "acceptance-cache": "ng build   -c=acceptance",
    "acceptance": "npm run get-build-date && ng build   -c=acceptance",
    "production": "ng cache clean && npm run get-build-date && ng build   -c=production",
    "build-scircabc-acc": "npm run get-build-date && ng build   -c=scircabc-acc",
    "build-scircabc-prod": "npm run get-build-date && ng build   -c=scircabc-prod",
    "build-shs-acceptance": "npm run get-build-date && ng build   -c=shs-acceptance",
    "build-shs-prod": "npm run get-build-date && ng build   -c=shs-prod",
    "build-enisa-acceptance": "npm run get-build-date && ng build   -c=enisa-acceptance",
    "build-enisa-prod": "npm run get-build-date && ng build   -c=enisa-prod",
    "build-olaf-localhost": "npm run get-build-date && ng build -c=olaf-localhost",
    "build-olaf-test": "npm run get-build-date && ng build -c=olaf-test",
    "build-olaf-acceptance": "npm run get-build-date && ng build   -c=olaf-acceptance",
    "build-olaf-prod": "npm run get-build-date && ng build   -c=olaf-prod",
    "build-comm-acceptance": "npm run get-build-date && ng build   -c=comm-acceptance",
    "build-comm-prod": "npm run get-build-date && ng build   -c=comm-prod",
    "ncu": "npx npm-check-updates",
    "ncu-u": "npx npm-check-updates -u",
    "browserslist": "npx browserslist",
    "check": "npx npm-check -u",
    "link-status": "npx link-status -p",
    "npx-http-serve": "npx http-serve ./dist/circabc -p 4200",
    "gen-version": "openapi-generator-cli version",
    "gen-version-list": "openapi-generator-cli version-manager list",
    "gen-templates": "rimraf ./openapi-templates && openapi-generator-cli author template -g typescript-angular -o ./openapi-templates",
    "circabc-api": "rimraf src/app/core/generated/circabc && openapi-generator-cli generate -i apis/openapi.yaml -g typescript-angular -o src/app/core/generated/circabc --additional-properties ngVersion=9.0.0,providedInRoot=true --template-dir ./openapi-templates  && npm run format-fix-gen",
    "ares-bridge-api": "rimraf src/app/core/generated/ares-bridge && openapi-generator-cli generate -i apis/ares-bridge.yaml -g typescript-angular -o src/app/core/generated/ares-bridge --additional-properties ngVersion=9.0.0,providedInRoot=true && npm run format-fix-gen",
    "eu-captcha-api": "rimraf src/app/core/generated/eu-captcha && openapi-generator-cli generate -i apis/eu-captcha.yaml -g typescript-angular -o src/app/core/generated/eu-captcha --additional-properties ngVersion=9.0.0,providedInRoot=true && npm run format-fix-gen",
    "tomcat-prod-it-nrw": "npm run get-build-date && ng build   -c=tomcat-prod-it-nrw && gzipper compress --verbose ./dist/circabc",
    "source-map": "ng build  --source-map",
    "source-map-explorer": "npx source-map-explorer dist/circabc/**/*.js",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "i18n:translate": "atj src/assets/i18n/en.json -e aws -m file",
    "spellcheck": "cspell /src/**/*.ts",
    "update-18": "ng update @angular/core@18 @angular/cli@18 --allow-dirty --force",
    "update-material-18": "ng update @angular/material@18 --allow-dirty --force",
    "migrate-material-15": "ng generate @angular/material:mdc-migration --force"
  },
  "private": true,
  "dependencies": {
    "@alfresco/js-api": "7.8.0",
    "@angular-devkit/core": "19.2.1",
    "@angular/animations": "19.2.1",
    "@angular/cdk": "19.2.2",
    "@angular/common": "19.2.1",
    "@angular/compiler": "19.2.1",
    "@angular/core": "19.2.1",
    "@angular/forms": "19.2.1",
    "@angular/material": "19.2.2",
    "@angular/platform-browser": "19.2.1",
    "@angular/platform-browser-dynamic": "19.2.1",
    "@angular/router": "19.2.1",
    "@angular/service-worker": "19.2.1",
    "@biomejs/biome": "1.9.4",
    "@jsverse/transloco": "7.5.1",
    "@microsoft/mgt": "4.4.0",
    "@primeng/themes": "19.0.9",
    "core-js": "3.41.0",
    "file-saver": "2.0.5",
    "font-awesome": "4.7.0",
    "ngx-cookie-service": "19.1.2",
    "ngx-extended-pdf-viewer": "22.3.9",
    "primeicons": "7.0.0",
    "primeng": "19.0.9",
    "quill": "2.0.2",
    "rxjs": "7.8.2",
    "ts-helpers": "1.1.2",
    "ubuntu-fontface": "0.1.13",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.1",
    "@angular-eslint/builder": "19.2.1",
    "@angular-eslint/eslint-plugin": "19.2.1",
    "@angular-eslint/eslint-plugin-template": "19.2.1",
    "@angular-eslint/schematics": "19.2.1",
    "@angular-eslint/template-parser": "19.2.1",
    "@angular/cli": "19.2.1",
    "@angular/compiler-cli": "19.2.1",
    "@compodoc/compodoc": "1.1.26",
    "@jsverse/transloco-keys-manager": "6.1.0",
    "@openapitools/openapi-generator-cli": "2.17.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/file-saver": "2.0.7",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "auto-translate-json-library": "1.4.8",
    "clang-format": "1.8.0",
    "crlf": "1.1.1",
    "cspell": "8.17.5",
    "eslint": "9.22.0",
    "eslint-formatter-visualstudio": "8.40.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.3",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-unicorn": "57.0.0",
    "gulp": "5.0.0",
    "gulp-clang-format": "1.0.27",
    "gulp-prettier": "6.0.0",
    "gzipper": "8.2.0",
    "jasmine-core": "5.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "jscpd": "4.0.5",
    "postcss-scss": "4.0.9",
    "postinstall-postinstall": "2.1.0",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "stylelint": "16.15.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  }
}
