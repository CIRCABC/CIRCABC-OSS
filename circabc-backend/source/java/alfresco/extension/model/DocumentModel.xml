<!-- Definition of Circabc Document Model. It extends the cm:??? contentModel -->
<model name="cd:circadocumentmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

   <!-- Optional meta-data about the model -->
   <description>CircaDocumentModel Model</description>
   <author>Trasys</author>
   <version>1.0</version>

   <!-- Imports are required to allow references to definitions in other models -->
   <imports>
   	  <!-- Import Alfresco Dictionary Definitions -->
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
      <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
   </imports>

   <!-- Introduction of new namespaces defined by this model -->
   <namespaces>
      <namespace uri="http://www.cc.cec/circabc/model/document/1.0" prefix="cd"/>
   </namespaces>

   <!-- Type and Aspect definitions go here -->

	<types>
		<type name="cd:hiddenContent">
			<title>Customization content</title>
			<parent>cm:cmobject</parent>
			<properties>
				<property name="cd:content">
					<type>d:content</type>
               		<mandatory>false</mandatory>
	               	<index enabled="false" />
				</property>
			</properties>
	 	</type>
	</types>

    <aspects>

    <aspect name="cd:circadocument">
		 <title>CircaDocument</title>
		<mandatory-aspects>
            <aspect>cd:bproperties</aspect>
            <aspect>cd:cproperties</aspect>
        	<aspect>cm:versionable</aspect>
         </mandatory-aspects>
	</aspect>


    <!-- Definition of new BProperties -->
    <aspect name="cd:bproperties">
         <title>BProperties</title>
         <properties>
         <property name="cd:security_ranking">
					<type>d:text</type>
					<mandatory>false</mandatory>
					<default>NORMAL</default>
					<constraints>
					<constraint name="cd:list_security_ranking" type="LIST">
				        <parameter name="allowedValues">
				            <list>
								<value>NORMAL</value>
								<value>SENSITIVE</value>
								<value>SPECIAL_HANDLING</value>	
				                <value>PUBLIC</value>
				                <value>INTERNAL</value>
				                <value>LIMITED</value>
				            </list>
				        </parameter>
				        <parameter name="caseSensitive"><value>false</value></parameter>
				     </constraint>
				     </constraints>
				</property>
				<property name="cd:expiration_date">
					<type>d:datetime</type>
					<mandatory>false</mandatory>
					<index enabled="true">
                  <atomic>true</atomic>
                  <stored>false</stored>
                  <tokenised>true</tokenised>
               </index>
				</property>
         </properties>
    </aspect>

    <!-- Definition of new CProperties -->
    <aspect name="cd:cproperties">
         <title>CProperties</title>
         <properties>
			<property name="cd:keyword">
               <title>Keywords</title>
               <type>d:category</type>
               <mandatory>false</mandatory>
               <multiple>true</multiple>
               <index enabled="true">
                  <atomic>true</atomic>
                  <stored>true</stored>
                  <tokenised>true</tokenised>
               </index>
            </property>
			<property name="cd:status">
					<type>d:text</type>
					<mandatory>true</mandatory>
					<default>DRAFT</default>
					<constraints>
					<constraint name="cd:list_status" type="LIST">
				        <parameter name="allowedValues">
				            <list>
				                <value>DRAFT</value>
				                <value>FINAL</value>
				                <value>RELEASE</value>
				            </list>
				        </parameter>
				        <parameter name="caseSensitive"><value>false</value></parameter>
				     </constraint>
				     </constraints>
				</property>
				<property name="cd:issue_date">
					<type>d:datetime</type>
					<mandatory>false</mandatory>
					<index enabled="true">
                  <atomic>true</atomic>
                  <stored>false</stored>
                  <tokenised>true</tokenised>
