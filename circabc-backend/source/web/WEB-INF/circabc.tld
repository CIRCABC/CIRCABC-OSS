	<tag>
		<name>actionLink</name>
		<tag-class>
			eu.cec.digit.circabc.web.ui.common.tag.ActionLinkTag
		</tag-class>
		<body-content>JSP</body-content>

		<description>
			The actionLink component provides the user with a clickable
			link, image or both to represent an action in the UI. The
			component provides standard action and actionListener
			command properties for developers to handle user
			interaction.
		</description>

		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>The text of the link</description>
		</attribute>

		<attribute>
			<name>rendered</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>action</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>actionListener</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>href</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				If specified, override the action and actionListener
				attribute. Used to render a standart link
			</description>
		</attribute>

		<attribute>
			<name>target</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>tooltip</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>To specify the title attribute</description>
		</attribute>

		<attribute>
			<name>image</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>To specify an image as the link</description>
		</attribute>

		<attribute>
			<name>showLink</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				If true and an image is specified, the value will be
				also shown.
			</description>
		</attribute>

		<attribute>
			<name>anchor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Must be used only to specified an anchor when using
				Action ou ActionListener attribute. To navigate inside
				the same page, use the href attribute starting with an
				"#". A leading "n" will be add to respect spec which
				anchor don't have to start with an number
			</description>
		</attribute>

		<attribute>
			<name>immediate</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>noDisplay</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>If true, no html code will be rendered. This is a tip to declare optional link into the JSF tree</description>
		</attribute>

		<attribute>
			<name>padding</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				The number of blanck spaces to insert between the image and the link
			</description>
		</attribute>

		<attribute>
         	<name>onclick</name>
         	<required>false</required>
         	<rtexprvalue>true</rtexprvalue>
      	</attribute>
      	
      	<attribute>
			<name>escape</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

