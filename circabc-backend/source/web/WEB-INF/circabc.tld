	<tag>
		<name>multiList</name>
		<tag-class>
			eu.cec.digit.circabc.web.ui.common.tag.data.MultiListTag
		</tag-class>
		<body-content>JSP</body-content>
		<description>
			The MultiList component is capable of rendering a list of
			java bean objects. A List of objects is bound to the
			component through the value attribute and components within
			the list can bind to a specific fields (column) through
			getter methods for each bean object within the list. His
			purpose is to render a simple list of element of several
			columns. Columns to be rendered have to be equals to
			attribute numColumn.
		</description>

		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>rendered</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>headerStyleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				It can be overrided if a styleClass is specified in the
				header column tag
			</description>
		</attribute>

		<attribute>
			<name>rowStyleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>altRowStyleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Array to be filled</description>
		</attribute>

		<attribute>
			<name>initialSortColumn</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>initialSortDescending</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>listConfig</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>pageSize</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Here pageSize is equals to the number of rows
			</description>
		</attribute>

		<attribute>
			<name>numColumn</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Specified the number of column within the list.
			</description>
		</attribute>
