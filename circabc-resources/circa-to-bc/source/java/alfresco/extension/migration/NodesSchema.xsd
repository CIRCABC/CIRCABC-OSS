<?xml version="1.0" encoding="UTF-8"?><xs:schema targetNamespace="https://circabc.europa.eu/Import/NodesSchema/1.0" xmlns:node="https://circabc.europa.eu/Import/NodesSchema/1.0" xmlns:iso="https://circabc.europa.eu/Import/LanguagesSchema/1.0" xmlns:perm="https://circabc.europa.eu/Import/PermissionsSchema/1.0" xmlns:props="https://circabc.europa.eu/Import/PropertiesSchema/1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">	<xs:import namespace="https://circabc.europa.eu/Import/LanguagesSchema/1.0" schemaLocation="LanguagesSchema.xsd"/>	<xs:import namespace="https://circabc.europa.eu/Import/PermissionsSchema/1.0" schemaLocation="PermissionsSchema.xsd"/>	<xs:import namespace="https://circabc.europa.eu/Import/PropertiesSchema/1.0" schemaLocation="PropertiesSchema.xsd"/>	<xs:simpleType name="uriString">		<xs:restriction base="xs:string">			<xs:whiteSpace value="collapse"/>			<xs:pattern value="file:.*|ftp:.*|http(s?):.*|classpath:.*|nntp:.*"/>		</xs:restriction>	</xs:simpleType>
	<xs:simpleType name="nodeReference">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value=".+://.+/.+"/>
		</xs:restriction>
	</xs:simpleType>	<xs:complexType name="baseNode" abstract="true"/>	<xs:complexType name="node" abstract="true">		<xs:complexContent>			<xs:extension base="node:baseNode">				<xs:sequence>
					<xs:element ref="node:nodeReference" minOccurs="0"/>					<xs:group ref="props:nodeProperties" minOccurs="0"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="titledNode" abstract="true">		<xs:complexContent>			<xs:extension base="node:node">				<xs:sequence>					<xs:group ref="props:titleProperties" minOccurs="0"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="namedNode" abstract="true">		<xs:complexContent>			<xs:extension base="node:titledNode">				<xs:attribute name="name" type="props:name" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="contentNode" abstract="true">		<xs:complexContent>			<xs:extension base="node:namedNode">
				<xs:sequence>
					<xs:element name="contentString" type="xs:string" minOccurs="0"/>
					<xs:element ref="props:versionLabel" />
				</xs:sequence>				<xs:attribute name="uri" type="node:uriString" use="optional"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="circabc">		<xs:complexContent>			<xs:extension base="node:titledNode">				<xs:sequence>					<xs:element ref="perm:circabcAdmin" minOccurs="0"/>					<xs:element ref="node:categoryHeader" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="categoryHeader">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>					<xs:element ref="node:category" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="category">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>					<xs:element ref="perm:categoryAdmin" minOccurs="0"/>					<xs:element ref="node:interestGroup" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>
	<xs:complexType name="interestGroup">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>					<xs:group ref="props:igProperties"/>					<xs:element ref="props:keywordDefinitions" minOccurs="0"/>					<xs:element ref="props:dynamicPropertyDefinitions" minOccurs="0"/>					<xs:element ref="perm:notifications" minOccurs="0"/>
					<xs:element ref="perm:applications" minOccurs="0"/>
					<xs:element ref="node:logoDefinitions" minOccurs="0"/>					<xs:element ref="node:information"/>					<xs:element ref="node:library"/>					<xs:element ref="node:directory"/>					<xs:element ref="node:events"/>					<xs:element ref="node:newsgroups"/>					<xs:element ref="node:surveys" minOccurs="0"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="information">		<xs:complexContent>			<xs:extension base="node:titledNode">				<xs:sequence>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:group ref="props:infProperties" minOccurs="0"/>					<xs:element ref="node:infSpace" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:infMLContent" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:infContent" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="library">		<xs:complexContent>			<xs:extension base="node:titledNode">				<xs:sequence>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="node:space" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:dossier" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:sharedSpacelink" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:mlContent" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:content" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:url" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:link" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="directory">		<xs:complexContent>			<xs:extension base="node:node">				<xs:sequence>					<xs:element ref="perm:guest" minOccurs="0" />					<xs:element ref="perm:registredUsers" minOccurs="0" />					<xs:element ref="perm:accessProfile"  minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="perm:importedProfile" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="events">		<xs:complexContent>			<xs:extension base="node:titledNode">				<xs:sequence>					<xs:group ref="props:eventServiceProperties" minOccurs="0"/>					<xs:element ref="node:meeting" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:event" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="newsgroups">		<xs:complexContent>			<xs:extension base="node:titledNode">				<xs:sequence>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="node:forum" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="surveys">		<xs:complexContent>			<xs:extension base="node:titledNode"/>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="informationContent">		<xs:complexContent>			<xs:extension base="node:contentNode">				<xs:sequence>					<xs:element name="versions" type="node:informationContentVersions" minOccurs="0" />					<xs:element ref="perm:notifications" minOccurs="0"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="libraryContent">		<xs:complexContent>			<xs:extension base="node:contentNode">				<xs:sequence>					<xs:group ref="props:bProperties" minOccurs="0"/>					<xs:group ref="props:cProperties" minOccurs="0"/>					<xs:element name="versions" type="node:libraryContentVersions" minOccurs="0" />					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="perm:libraryUserRights" minOccurs="0"/>
					<xs:element ref="node:discussions" minOccurs="0" />				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>
	<xs:complexType name="simpleContent">
		<xs:complexContent>
			<xs:extension base="node:contentNode" />
		</xs:complexContent>
	</xs:complexType>	<xs:complexType name="informationContentVersion">		<xs:complexContent>			<xs:extension base="node:contentNode">				<xs:sequence>					<xs:element ref="props:versionNote" minOccurs="0"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="libraryContentVersion">		<xs:complexContent>			<xs:extension base="node:contentNode">				<xs:sequence>					<xs:group ref="props:bProperties" minOccurs="0"/>					<xs:group ref="props:cProperties" minOccurs="0"/>					<xs:element ref="props:versionNote" minOccurs="0"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="informationContentVersions">		<xs:sequence>			<xs:element name="version" type="node:informationContentVersion" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="libraryContentVersions">		<xs:sequence>			<xs:element name="version" type="node:libraryContentVersion" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="informationMLContent">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>
					<xs:group ref="props:mlContentProperties" minOccurs="0"/>					<xs:element name="translation" type="node:informationTranslation" maxOccurs="unbounded">						<xs:unique name="uniqueNames_info_translation_disuccsions">							<xs:selector xpath="./*"/>							<xs:field xpath="@name"/>						</xs:unique>					</xs:element>				</xs:sequence>				<xs:attribute name="pivotLang" type="props:pivotLang" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="libraryMLContent">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>
					<xs:group ref="props:mlContentProperties" minOccurs="0"/>					<xs:group ref="props:bProperties" minOccurs="0"/>					<xs:element name="translation" type="node:libraryTranslation" maxOccurs="unbounded">						<xs:unique name="uniqueNames_lib_translation_disuccsions">							<xs:selector xpath="./*"/>							<xs:field xpath="@name"/>						</xs:unique>					</xs:element>
					<xs:element ref="node:discussions" minOccurs="0" />				</xs:sequence>				<xs:attribute name="pivotLang" type="props:pivotLang" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="informationTranslation">		<xs:complexContent>			<xs:extension base="node:contentNode">				<xs:sequence>					<xs:element name="versions" type="node:informationTranslationVersions" minOccurs="0" />					<xs:element ref="perm:notifications" minOccurs="0"/>				</xs:sequence>				<xs:attribute name="lang" type="props:lang" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="libraryTranslation">		<xs:complexContent>			<xs:extension base="node:contentNode">				<xs:sequence>					<xs:group ref="props:cProperties" minOccurs="0"/>					<xs:element name="versions" type="node:libraryTranslationVersions" minOccurs="0" />					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="perm:libraryUserRights" minOccurs="0"/>
					<xs:element ref="node:discussions" minOccurs="0" />				</xs:sequence>				<xs:attribute name="lang" type="props:lang" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="informationTranslationVersion">		<xs:complexContent>			<xs:extension base="node:contentNode">				<xs:sequence>					<xs:element ref="props:versionNote" minOccurs="0"/>				</xs:sequence>				<xs:attribute name="lang" type="props:lang" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="libraryTranslationVersion">		<xs:complexContent>			<xs:extension base="node:contentNode">				<xs:sequence>					<xs:group ref="props:cProperties" minOccurs="0"/>					<xs:element ref="props:versionNote" minOccurs="0"/>				</xs:sequence>				<xs:attribute name="lang" type="props:lang" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="informationTranslationVersions">		<xs:sequence>			<xs:element name="version" type="node:informationTranslationVersion" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="libraryTranslationVersions">		<xs:sequence>			<xs:element name="version" type="node:libraryTranslationVersion" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="dossier">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="perm:libraryUserRights" minOccurs="0"/>					<xs:element ref="node:url" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:link" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="node:discussions" minOccurs="0" />				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="forum">		<xs:complexContent>			<xs:extension base="node:namedNode" >				<xs:sequence>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="perm:newsgroupUserRights" minOccurs="0"/>					<xs:element ref="node:forum" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:topic" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>
				<xs:attribute name="moderated" type="xs:boolean" default="false" use="optional"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="informationSpace">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>
					<xs:element ref="perm:informationUserRights" minOccurs="0"/>					<xs:element ref="node:infSpace" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:infMLContent" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:infContent" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="link">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="perm:libraryUserRights" minOccurs="0"/>				</xs:sequence>				<xs:attribute name="reference" type="props:pathReference" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="sharedSpacelink">		<xs:complexContent>			<xs:extension base="node:link" />		</xs:complexContent>	</xs:complexType>	<xs:complexType name="appointment" abstract="true">		<xs:sequence>			<xs:group ref="props:appointmentProperties"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="meeting">		<xs:complexContent>			<xs:extension base="node:appointment">				<xs:sequence>					<xs:group ref="props:meetingProperties"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="event">		<xs:complexContent>			<xs:extension base="node:appointment">				<xs:sequence>					<xs:group ref="props:eventProperties"/>				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="librarySpace">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>					<xs:group ref="props:librarySpaceProperties"/>					<xs:element ref="props:shared" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="perm:libraryUserRights" minOccurs="0"/>					<xs:element ref="node:space" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:dossier" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:sharedSpacelink" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:mlContent" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:content" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:url" minOccurs="0" maxOccurs="unbounded"/>					<xs:element ref="node:link" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="node:discussions" minOccurs="0" />				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="message">		<xs:complexContent>			<xs:extension base="node:node">				<xs:sequence>
					<xs:group ref="props:moderated" minOccurs="0"/>
					<xs:element ref="perm:notifications" minOccurs="0" />					<xs:element ref="perm:newsgroupUserRights" minOccurs="0" />
					<xs:element name="content" type="xs:string" minOccurs="0" />
					<xs:element name="reply" type="node:message" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="relatedDocument" type="node:simpleContent" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="relatedLinks" type="node:link" minOccurs="0" maxOccurs="unbounded" />				</xs:sequence>
				<xs:attribute name="uri" type="node:uriString" use="optional"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="topic">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>
					<xs:group ref="props:bProperties" minOccurs="0"/>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="perm:newsgroupUserRights" minOccurs="0"/>					<xs:element ref="node:message" maxOccurs="unbounded"/>				</xs:sequence>
				<xs:attribute name="moderated" type="xs:boolean" default="false" use="optional"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>
	<xs:complexType name="discussions">
		<xs:complexContent>
			<xs:extension base="node:namedNode">
				<xs:sequence>
					<xs:element ref="node:topic" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>	<xs:complexType name="url">		<xs:complexContent>			<xs:extension base="node:namedNode">				<xs:sequence>					<xs:element ref="props:target"/>					<xs:element ref="perm:notifications" minOccurs="0"/>					<xs:element ref="perm:libraryUserRights" minOccurs="0"/>
					<xs:element ref="node:discussions" minOccurs="0" />				</xs:sequence>			</xs:extension>		</xs:complexContent>	</xs:complexType>
	<xs:element name="logo">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="uri" type="node:uriString" use="required"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="logoDefinitions">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="node:logo"/>
			</xs:sequence>
			<xs:attribute name="defaultLogo" type="xs:string" use="optional"/>
		</xs:complexType>
		<xs:unique name="logo_definitions_property_name">
			<xs:selector xpath="./*"/>
			<xs:field xpath="@name"/>
		</xs:unique>
	</xs:element>	<!--		Top level nodes	-->	<xs:element name="circabc" type="node:circabc">		<xs:unique name="uniqueNames_header">			<xs:selector xpath="./*"/>			<xs:field xpath="@name"/>		</xs:unique>		<xs:unique name="uniqueNames_categories">			<xs:selector xpath="./node:categoryHeader/node:category"/>			<xs:field xpath="@name"/>		</xs:unique>	</xs:element>	<xs:element name="categoryHeader" type="node:categoryHeader"/>	<xs:element name="category" type="node:category">		<xs:unique name="uniqueNames_ig">			<xs:selector xpath="./*"/>			<xs:field xpath="@name"/>		</xs:unique>	</xs:element>	<xs:element name="interestGroup" type="node:interestGroup"/>	<!--		Interest Group Services	-->	<xs:element name="information" type="node:information">		<xs:unique name="uniqueNames_inf_childs">			<xs:selector xpath="./*|./node:infMLContent/node:translation"/>			<xs:field xpath="@name"/>		</xs:unique>	</xs:element>	<xs:element name="library" type="node:library">		<xs:unique name="uniqueNames_lib_childs">			<xs:selector xpath="./*|./node:mlContent/node:translation"/>			<xs:field xpath="@name"/>		</xs:unique>	</xs:element>	<xs:element name="directory" type="node:directory">		<xs:unique name="uniqueNames_profiles">			<xs:selector xpath="./*"/>			<xs:field xpath="@name"/>		</xs:unique>	</xs:element>	<xs:element name="events" type="node:events"/>	<xs:element name="newsgroups" type="node:newsgroups">		<xs:unique name="uniqueNames_forums">			<xs:selector xpath="./*"/>			<xs:field xpath="@name"/>		</xs:unique>	</xs:element>	<xs:element name="surveys" type="node:surveys"/>	<xs:element name="infMLContent" type="node:informationMLContent">		<xs:unique name="uniqueNames_inf_translation">			<xs:selector xpath="./*"/>			<xs:field xpath="@name"/>		</xs:unique>		<xs:unique name="uniqueLang_inf_translation">			<xs:selector xpath="./*"/>			<xs:field xpath="@lang"/>		</xs:unique>	</xs:element>	<xs:element name="mlContent" type="node:libraryMLCon