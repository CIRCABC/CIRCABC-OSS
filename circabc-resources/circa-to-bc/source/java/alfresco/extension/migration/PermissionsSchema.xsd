<?xml version="1.0" encoding="UTF-8"?><xs:schema targetNamespace="https://circabc.europa.eu/Import/PermissionsSchema/1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:user="https://circabc.europa.eu/Import/UsersSchema/1.0" xmlns:props="https://circabc.europa.eu/Import/PropertiesSchema/1.0" xmlns:perm="https://circabc.europa.eu/Import/PermissionsSchema/1.0" elementFormDefault="qualified">	<xs:import namespace="https://circabc.europa.eu/Import/PropertiesSchema/1.0" schemaLocation="PropertiesSchema.xsd"/>	<xs:import namespace="https://circabc.europa.eu/Import/UsersSchema/1.0" schemaLocation="UsersSchema.xsd"/>	<xs:simpleType name="profileName">		<xs:restriction base="xs:string">			<xs:minLength value="3"/>			<xs:maxLength value="128"/>			<xs:whiteSpace value="collapse"/>			<xs:pattern value="ALL_CIRCA_USERS|([a-z]|[A-Z]|[0-9]|:)+"/>		</xs:restriction>		<!-- at least 3 alphanumeric chars  -->	</xs:simpleType>	<xs:element name="profile" type="perm:profileName"/>	<xs:simpleType name="simpleInformationPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="InfAccess"/>			<xs:enumeration value="InfNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="informationPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="InfAdmin"/>			<xs:enumeration value="InfManage"/>			<xs:enumeration value="InfAccess"/>			<xs:enumeration value="InfNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="simpleLibraryPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="LibAccess"/>			<xs:enumeration value="LibNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="libraryPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="LibAdmin"/>			<xs:enumeration value="LibFullEdit"/>			<xs:enumeration value="LibManageOwn"/>			<xs:enumeration value="LibEditOnly"/>			<xs:enumeration value="LibAccess"/>			<xs:enumeration value="LibNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="simpleEventPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="EveAccess"/>			<xs:enumeration value="EveNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="eventPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="EveAdmin"/>			<xs:enumeration value="EveAccess"/>			<xs:enumeration value="EveNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="simpleNewsgroupPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="NwsAccess"/>			<xs:enumeration value="NwsNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="newsgroupPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="NwsAdmin"/>			<xs:enumeration value="NwsModerate"/>			<xs:enumeration value="NwsPost"/>			<xs:enumeration value="NwsAccess"/>			<xs:enumeration value="NwsNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="simpleDirectoryPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="DirAccess"/>			<xs:enumeration value="DirNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="directoryPermissions">		<xs:restriction base="xs:string">			<xs:enumeration value="DirAdmin"/>			<xs:enumeration value="DirManageMembers"/>			<xs:enumeration value="DirAccess"/>			<xs:enumeration value="DirNoAccess"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="notificationStatus">		<xs:restriction base="xs:string">			<xs:enumeration value="Suscribe"/>			<xs:enumeration value="Unsuscribe"/>			<xs:enumeration value="Default"/>		</xs:restriction>	</xs:simpleType>
	<xs:complexType name="informationPermissionItem">
		<xs:sequence>
			<xs:element name="right" type="perm:informationPermissions"/>
			<xs:choice>
				<xs:element name="user" type="user:userId"/>
				<xs:element ref="perm:profile"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>	<xs:complexType name="libraryPermissionItem">		<xs:sequence>			<xs:element name="right" type="perm:libraryPermissions"/>			<xs:choice>				<xs:element name="user" type="user:userId"/>				<xs:element ref="perm:profile"/>			</xs:choice>		</xs:sequence>	</xs:complexType>	<xs:complexType name="newsgroupPermissionItem">		<xs:sequence>			<xs:element name="right" type="perm:newsgroupPermissions"/>			<xs:choice>				<xs:element name="user" type="user:userId"/>				<xs:element ref="perm:profile"/>			</xs:choice>		</xs:sequence>	</xs:complexType>	<xs:element name="libraryUserRights">		<xs:complexType>			<xs:sequence minOccurs="0" maxOccurs="unbounded">				<xs:element name="permission" type="perm:libraryPermissionItem"/>			</xs:sequence>			<xs:attribute name="inherit" type="xs:boolean" default="true"/>		</xs:complexType>	</xs:element>
	<xs:element name="informationUserRights">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="permission" type="perm:informationPermissionItem"/>
			</xs:sequence>
			<xs:attribute name="inherit" type="xs:boolean" default="true"/>
		</xs:complexType>
	</xs:element>	<xs:element name="newsgroupUserRights">		<xs:complexType>			<xs:sequence minOccurs="0" maxOccurs="unbounded">				<xs:element name="permission" type="perm:newsgroupPermissionItem"/>			</xs:sequence>			<xs:attribute name="inherit" type="xs:boolean" default="true"/>		</xs:complexType>	</xs:element>	<xs:complexType name="notificationItem">		<xs:sequence>			<xs:element name="status" type="perm:notificationStatus" default="Default"/>			<xs:choice>				<xs:element name="user" type="user:userId"/>				<xs:element ref="perm:profile"/>			</xs:choice>		</xs:sequence>	</xs:complexType>	<xs:element name="notifications">		<xs:complexType>			<xs:sequence minOccurs="0" maxOccurs="unbounded">				<xs:element name="notification" type="perm:notificationItem"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="circabcAdmin">		<xs:complexType>			<xs:choice minOccurs="0" maxOccurs="unbounded">				<xs:element name="user" type="user:userId"/>			</xs:choice>		</xs:complexType>	</xs:element>	<xs:element name="categoryAdmin">		<xs:complexType>			<xs:choice minOccurs="0" maxOccurs="unbounded">				<xs:element name="user" type="user:userId"/>			</xs:choice>		</xs:complexType>	</xs:element>	<xs:element name="applications">		<xs:complexType>			<xs:choice minOccurs="0" maxOccurs="unbounded">				<xs:element ref="perm:application"/>			</xs:choice>		</xs:complexType>	</xs:element>	<xs:element name="application">		<xs:complexType>			<xs:sequence>				<xs:element name="date" type="xs:date"/>				<xs:element name="user" type="user:userId"/>				<xs:element name="message" type="xs:string"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:complexType name="emptyAccessProfile">		<xs:sequence>			<xs:group ref="props:titleProperties" minOccurs="0"/>			<xs:element name="informationPermission" type="perm:informationPermissions" default="InfNoAccess" minOccurs="0"/>			<xs:element name="libraryPermission" type="perm:libraryPermissions" default="LibNoAccess" minOccurs="0"/>			<xs:element name="directoryPermission" type="perm:directoryPermissions" default="DirNoAccess" minOccurs="0"/>			<xs:element name="eventPermission" type="perm:eventPermissions" default="EveNoAccess" minOccurs="0"/>			<xs:element name="newsgroupPermission" type="perm:newsgroupPermissions" default="NwsNoAccess" minOccurs="0"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="accessProfile">		<xs:complexContent>			<xs:extension base="perm:emptyAccessProfile">				<xs:sequence>					<xs:element name="user" type="user:userId" minOccurs="0" maxOccurs="unbounded"/>				</xs:sequence>				<xs:attribute name="name" type="perm:profileName" use="required"/>				<xs:attribute name="exported" type="xs:boolean" use="optional" default="false"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="globalAccessProfile">		<xs:sequence>			<xs:group ref="props:titleProperties" minOccurs="0"/>			<xs:element name="informationPermission" type="perm:simpleInformationPermissions" default="InfNoAccess"/>			<xs:element name="libraryPermission" type="perm:simpleLibraryPermissions" default="LibNoAccess"/>			<xs:element name="directoryPermission" type="perm:simpleDirectoryPermissions" default="DirNoAccess"/>			<xs:element name="eventPermission" type="perm:simpleEventPermissions" default="EveNoAccess"/>			<xs:element name="newsgroupPermission" type="perm:simpleNewsgroupPermissions" default="NwsNoAccess"/>		</xs:sequence>		<xs:attribute name="visibility" type="xs:boolean" use="required"/>	</xs:complexType>	<xs:complexType name="importedProfile">		<xs:complexContent>			<xs:extension base="perm:emptyAccessProfile">				<xs:attribute name="targetInterestgroup" type="props:nodeName" use="required"/>				<xs:attribute name="targetProfile" type="perm:profileName" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="guest">		<xs:complexContent>			<xs:extension base="perm:globalAccessProfile"/>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="registredUsers">		<xs:complexContent>			<xs:extension base="perm:globalAccessProfile"/>		</xs:complexContent>	</xs:complexType>	<xs:element name="guest" type="perm:guest"/>	<xs:element name="registredUsers" type="perm:registredUsers"/>	<xs:element name="accessProfile" type="perm:accessProfile"/>	<xs:element name="importedProfile" type="perm:importedProfile"/></xs:schema>